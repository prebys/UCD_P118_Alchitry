// This file has all the pins associated with the Alchitry-Au and Alchitry-Io
// boards used in UC Davis P116B.  Just copy the ones that are needed.
//
//  Alchitry-Au board
//
//   28-JAN-2020	E.Prebys		Original, based on the files au.acf and io.acf, 
// 									distributed with Alchitry Lab
//
clock clk 100MHz;
////////////////////////////////////////////////////////
// Here are the devices on the Alchitry-Au board itself
pin clk CLOCK;
// Onboard LEDs
pin led[0] LED0;
pin led[1] LED1;
pin led[2] LED2;
pin led[3] LED3;
pin led[4] LED4;
pin led[5] LED5;
pin led[6] LED6;
pin led[7] LED7;

//pin rst_n RESET;      // Reset button (active low)
